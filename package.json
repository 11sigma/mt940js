{
  "name": "mt940js",
  "version": "1.4.0-beta.0",
  "description": "javascript mt940 bank statement parser",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "testw": "mocha --watch",
    "testcov": "nyc mocha",
    "lint": "eslint lib test cli.js index.js",
    "doc": "jsdoc -c jsdoc.json",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coverage": "nyc --reporter=html --reporter=text mocha",
    "build:umd": "npx webpack index.js -o dist/index.min.js -p --output-library mt940js --output-library-target umd --devtool source-map",
    "prebuild": "rimraf dist",
    "build": "npm run build:umd",
    "prepublishOnly": "npm run test && npm run lint && npm run build"
  },
  "keywords": [
    "mt940",
    "swift",
    "bank",
    "statement",
    "parser"
  ],
  "files": [
    "README.md",
    "changelog.txt",
    "index.js",
    "cli.js",
    "lib",
    "jsdoc.json",
    "dist"
  ],
  "author": "Alexander Tsybulsky",
  "license": "Apache-2.0",
  "homepage": "https://github.com/a-fas/mt940js.git",
  "repository": "https://github.com/a-fas/mt940js.git",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.3.0",
    "jsdoc": "^3.6.4",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2"
  }
}
